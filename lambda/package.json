{
  "name": "disi-ai-file-processor",
  "version": "1.0.0",
  "description": "AWS Lambda function for processing files",
  "main": "index.js",
  "scripts": {
    "build": "esbuild index.ts --bundle --platform=node --target=node20 --outfile=dist/index.js",
    "deploy": "aws lambda update-function-code --function-name disi-ai-file-processor --zip-file fileb://dist/function.zip"
  },
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/client-textract": "^3.0.0",
    "@aws-sdk/client-transcribe": "^3.0.0",
    "@upstash/redis": "^1.0.0",
    "@upstash/vector": "^1.0.0",
    "convex": "^1.0.0",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.0",
    "@types/node": "^20.0.0",
    "esbuild": "^0.19.0",
    "typescript": "^5.0.0"
  }
}
